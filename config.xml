<?xml version='1.0' encoding='utf-8'?>
<widget id="com.manywho.cordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>ManyWho</name>
    <description>
        ManyWho application template.
    </description>
    <author email="support@manywho.com" href="https://manywho.com">
        ManyWho Team
    </author>
    <content src="index.html" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <access origin="*" />
    <allow-navigation href="file:*" />
    <allow-navigation href="https://flow.manywho.com/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="file:*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <icon height="180" src="res/ios/icon/icon180x180.png" width="180" />
        <icon height="60" src="res/ios/icon/icon60x60.png" width="60" />
        <icon height="120" src="res/ios/icon/icon120x120.png" width="120" />
        <icon height="76" src="res/ios/icon/icon76x76.png" width="76" />
        <icon height="152" src="res/ios/icon/icon152x152.png" width="152" />
        <icon height="40" src="res/ios/icon/icon40x40.png" width="40" />
        <icon height="80" src="res/ios/icon/icon80x80.png" width="80" />
        <icon height="57" src="res/ios/icon/icon57x57.png" width="57" />
        <icon height="114" src="res/ios/icon/icon114x114.png" width="114" />
        <icon height="72" src="res/ios/icon/icon72x72.png" width="72" />
        <icon height="144" src="res/ios/icon/icon144x144.png" width="144" />
        <icon height="29" src="res/ios/icon/icon29x29.png" width="29" />
        <icon height="58" src="res/ios/icon/icon58x58.png" width="58" />
        <icon height="50" src="res/ios/icon/icon50x50.png" width="50" />
        <icon height="100" src="res/ios/icon/icon100x100.png" width="100" />
        <splash height="480" src="res/ios/screen/screen320x320.png" width="320" />
        <splash height="960" src="res/ios/screen/screen640x960.png" width="640" />
        <splash height="1024" src="res/ios/screen/screen768x1024.png" width="768" />
        <splash height="2048" src="res/ios/screen/screen1536x2048.png" width="1536" />
        <splash height="768" src="res/ios/screen/screen1024x768.png" width="1024" />
        <splash height="1536" src="res/ios/screen/screen2048x1536.png" width="2048" />
        <splash height="1136" src="res/ios/screen/screen640x1136.png" width="640" />
        <splash height="1334" src="res/ios/screen/screen750x1334.png" width="750" />
        <splash height="2208" src="res/ios/screen/screen1242x2208.png" width="1242" />
        <splash height="1242" src="res/ios/screen/screen2208x1242.png" width="2208" />
    </platform>
    <engine name="android" spec="^6.2.3" />
    <engine name="ios" spec="~4.3.1" />
    <plugin name="cordova-plugin-browsersync" spec="^0.1.6" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
